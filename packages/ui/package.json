{
  "name": "@rightbrainai/ui",
  "version": "0.0.3",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./global.css": "./styles/global.css",
    "./card": "./components/card.tsx",
    "./button": "./components/ui/button.tsx"
  },
  "scripts": {
    "build:deprecated": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
