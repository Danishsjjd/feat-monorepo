{
  "name": "rightbrainai",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "dotenv -- turbo run dev",
    "build": "turbo run build",
    "lint": "turbo lint --cache-dir=.turbo",
    "type-check": "turbo type-check --cache-dir=.turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "dotenv-cli": "^7.4.1",
    "turbo": "^1.13.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.15.4"
}
